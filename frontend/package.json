{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run lint && sonar-scanner && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "jest --coverage --passWithNoTests",
    "test:unit": "jest --coverage --passWithNoTests --watch --testPathPattern=src/__tests__/unit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:all": "npm run lint && npm run test && npm run cy:run",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.12.1",
    "@mui/x-date-pickers": "^6.2.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "18.14.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "babel-jest": "^29.5.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "framer-motion": "^10.8.5",
    "install": "^0.13.0",
    "jest-dom": "^4.0.0",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-circular-slider-svg": "^0.2.0",
    "react-dom": "18.2.0",
    "react-loader-spinner": "^5.3.4",
    "react-redux": "^8.0.5",
    "ts-jest": "^29.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-syntax-decorators": "^7.21.0",
    "@babel/preset-typescript": "^7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "cypress": "^12.8.1",
    "eslint": "^8.36.0",
    "eslint-config-fbjs": "^4.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-ft-flow": "^2.0.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "hermes-eslint": "^0.10.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "sass": "^1.58.3",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.module\\.scss$": "<rootDir>/styleMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)?$": [
        "ts-jest",
        {
          "tsconfig": "./tsconfig.test.json"
        }
      ]
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts",
      "@testing-library/jest-dom/extend-expect"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-circular-slider-svg)/"
    ]
  },
  "volta": {
    "node": "20.0.0"
  }
}
